{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["components/*"],
      "@const/*": ["const/*"],
      "@lib/*": ["lib/*"],
      "@config": ["config"]
    },
    "plugins": [
      {
        "name": "typescript-styled-plugin",
        "validate": false,
        "lint": {
          "validProperties": ["debug"]
        }
      }
    ]
  },
  "exclude": ["node_modules"]
}
